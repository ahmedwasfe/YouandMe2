<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ahmet.postphotos.UI.UserData.User.AllUsers">

    <include layout="@layout/app_tool_bar"
        android:id="@+id/app_UsersPage_toolbar"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerAllUsers"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/app_UsersPage_toolbar">

    </android.support.v7.widget.RecyclerView>


</RelativeLayout>
